{
  "manifest_version": 2,
  "name": "TradingView Image Copy",
  "author": "Jesse Chen",
  "version": "1.0.1",
  "description": "TradingView Image Copy",
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://*.tradingview.com/chart/*"],
      "js": ["index.js"]
    }
  ]
}
